JC,KAAA,CAAA,YAID;;aAJC,KAAA,CAAA,YAAA,GAAY,M;MAIb;AAHP,IAAA,GAAG,EAAE,KAAA,CAAA,UAGE;AAFP,IAAA,GAAG,EAAE,KAAA,CAAA,UAEE;AADP,IAAA,QAAQ,EAAE;AACH,GANV,E,IAAA,E;;AAAA,I,4BAAA,CAOI,CARN,CAYI,EAHJ,YAAA,CAEM,KAFN,EAEM;AAFA,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,aAAA,IAAA,QAAA,CAAA,aAAA,OAAA,QAAA,YAAF;AAAA,KAAA,CAEL;AAFsB,IAAA,KAAuB,EAAvB;AAAA,gBAAA;AAAA;AAEtB,GAFN,E,CACE,YAAA,CAA4C,iBAA5C,EAA4C;AAApC,IAAA,IAAI,EAAC,aAA+B;AAAjB,IAAA,IAAI,EAAC;AAAY,GAA5C,C,CADF,CAGI,CAjEN,CAkEI,CAnEN,C,EAqEA,YAAA,CAA6C,mBAA7C,EAA6C;AAAnC,IAAA,KAAK,EAAC;AAA6B,GAA7C,C","sourcesContent":["<template style=\"position:relative;\">\n  <div class=\"player_block\">\n    <div class=\"player\">\n      <div class=\"silder_progress_block\">\n        <Slider\n          tooltip=\"none\"\n          v-model=\"value_volume\"\n          :max=\"max_volume\"\n          :min=\"min_volume\"\n          :tooltips=\"false\"\n        ></Slider>\n      </div>\n      <div class=\"player_image_box hide_on_576\">\n        <img\n          class=\"player_image\"\n          src=\"https://en.pressemag.fr/wp-content/uploads/2021/01/DaBaby-Press-Photo-1_Photo-Credit_Jackie-Dimailig.jpg\"\n          alt=\"\"\n        />\n      </div>\n      <div class=\"title\">\n        <div><span class=\"title\">Dababy type beat</span></div>\n        <div style=\"font-size: 11px; margin-top: 5px; color: #ccc\">\n          TheTrackMonster\n        </div>\n      </div>\n      <div style=\"cursor: pointer\" class=\"hide_on_576\">\n        <unicon name=\"share-alt\" fill=\"#42b983\" />\n      </div>\n      <div class=\"btn_buy_player hide_on_576\">\n        <div class=\"hide_on_576\">\n          <div>\n            <unicon name=\"shopping-bag\" width=\"15\" height=\"15\" fill=\"white\" />\n          </div>\n        </div>\n        <div class=\"hide_on_576\">\n          <span style=\"color: white; font-size: 15px\">$29.99</span>\n        </div>\n      </div>\n      <div class=\"play_zone\">\n        <div>\n          <unicon name=\"step-backward\" width=\"15\" height=\"15\" fill=\"#42b983\" />\n        </div>\n        <div>\n          <unicon name=\"play\" width=\"35\" height=\"35\" fill=\"#42b983\" />\n        </div>\n        <div>\n          <unicon name=\"skip-forward\" width=\"15\" height=\"15\" fill=\"#42b983\" />\n        </div>\n        <div>\n          <unicon name=\"repeat\" width=\"18\" height=\"18\" fill=\"#42b983\" />\n        </div>\n      </div>\n      <div class=\"hide_on_768\">\n        <unicon name=\"volume\" width=\"18\" height=\"18\" fill=\"#42b983\" />\n      </div>\n      <div style=\"width: 100px\" class=\"hide_on_768\">\n        <Slider\n          tooltip=\"none\"\n          v-model=\"value_volume\"\n          :max=\"max_volume\"\n          :min=\"min_volume\"\n          :tooltips=\"false\"\n        ></Slider>\n      </div>\n      <div @click=\"open_playlist\" style=\"cursor: pointer\">\n        <unicon name=\"list-ui-alt\" fill=\"#42b983\" />\n      </div>\n    </div>\n  </div>\n\n  <Playlist class=\"play_list_block\"></Playlist>\n</template>\n<style scoped>\n.silder_progress_block {\n  width: 100%;\n  z-index: 10000000;\n  position: absolute;\n  top: 0;\n}\n.play_list_block {\n  position: fixed;\n  bottom: 0;\n  height: 0px;\n  transition: ease-in-out 0.5s all;\n  border-top-width: solid;\n  border-width: 1px;\n  border-color: #42b983;\n  background: white;\n  width: 100%;\n  z-index: 10000000;\n  overflow: scroll;\n}\n.play_zone {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 40%;\n}\n.play_zone > div {\n  margin: 0px 10px;\n}\n.btn_buy_player {\n  background: #42b983;\n  display: flex;\n  padding: 2px 15px 5px 15px;\n  align-items: center;\n  justify-content: center;\n  border-radius: 3px;\n  cursor: pointer;\n}\n.btn_buy_player > div {\n  margin: 0 2px 0 2px;\n}\n.player {\n  background: white;\n  width: 100%;\n  box-shadow: rgba(50, 50, 93, 0.25) 0px 2px 5px -1px,\n    rgba(0, 0, 0, 0.3) 0px 1px 3px -1px;\n  padding: 30px 0px 10px 0px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  transition: ease-in-out 0.5s all;\n  z-index: 100000;\n  position: fixed;\n  bottom: 0;\n}\n.player_block {\n  position: fixed;\n  bottom: 0;\n  width: 100%;\n  z-index: 10000;\n}\n.player > div {\n  margin: 0px 15px 0px 15px;\n}\n.player_image {\n  object-fit: cover;\n  width: 50px;\n  height: 50px;\n}\n@media only screen and (max-width: 768px) {\n  .hide_on_768 {\n    display: none;\n  }\n}\n@media only screen and (max-width: 576px) {\n  .hide_on_576 {\n    display: none;\n  }\n  .title {\n    font-size: 13px;\n  }\n}\n</style>\n<style src=\"@vueform/slider/themes/default.css\"></style>\n<script>\nimport Slider from \"@vueform/slider\";\nimport Playlist from \"@/components/Playlist.vue\";\nexport default {\n  name: \"Player\",\n  components: {\n    Slider,\n    Playlist,\n  },\n  data() {\n    return {\n      value_volume: 10,\n      max_volume: 10,\n      min_volume: 0,\n      is_open: false,\n    };\n  },\n  mounted() {},\n  methods: {\n    open_playlist() {\n      if (this.is_open == false) {\n        document.querySelector(\".player\").style.bottom = 250 + \"px\";\n        this.is_open = true;\n        document.querySelector(\".play_list_block\").style.height = 250 + \"px\";\n        document.querySelector(\".play_list_block\").style.borderStyle = \"solid\";\n      } else {\n        document.querySelector(\".player\").style.bottom = 0;\n        document.querySelector(\".play_list_block\").style.height = 0 + \"px\";\n        document.querySelector(\".play_list_block\").style.borderStyle = \"none\";\n        this.is_open = false;\n      }\n    },\n  },\n};\n</script>\n"],"sourceRoot":""}]}